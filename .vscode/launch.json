{
    "name": "Socket.Server (Debug)",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Socket.Server (Debug)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Socket.Server/bin/Debug/net9.0/Socket.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Socket.Server",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/Socket.Server/Views"
            }
        }
    ],
    
}